$(document).ready(function(){
	var myDate = new Date();
	var month = myDate.getMonth() + 1;
	var m;
	if(month > 0 && month < 10){
		m = "0"+month;
	} else {
	   m = month;
	}
	var prettyDate = myDate.getFullYear() + '-' + m + '-' + myDate.getDate();
	//$("#MoveIn").datepicker({dateFormat : 'yy-mm-dd'}).val(date);
	$("#MoveIn").val(prettyDate);
	
	
	$.ajax({
		type : 'GET',
		url : "/ApartmentPortal/rest/services/apartmentno/"+prettyDate, 
		success : function(data){		
				 $('#aptType').append(data);
		},
		error : function(XMLHttpRequest, textStatus, errorThrown) {
				alert("Ajax error in fetching floor plan list.");
			}
		});	
	
	
});

function getApts(){
	var dt = $("#MoveIn").val();
	$.ajax({
		type : 'GET',
		url : "/ApartmentPortal/rest/services/apartmentno/"+dt, 
		success : function(data){
				 
				 $('#aptType').empty().append(data);
		},
		error : function(XMLHttpRequest, textStatus, errorThrown) {
				alert("Ajax error in fetching floor plan list.");
			}
		});	
}

function abcd(){
	var apNum =document.getElementById("aptType");
	var dt = document.getElementById("MoveIn");
	document.getElementById("aptType1").value = apNum;
	document.getElementById("MoveIn1").value = dt;
	
	
}

</script>
